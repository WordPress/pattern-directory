{
	"name": "wporg-pattern-directory",
	"version": "1.0.0",
	"description": "Creates a directory to manage block patterns.",
	"author": "The WordPress Meta Team",
	"license": "GPL-2.0-or-later",
	"scripts": {
		"build": "wp-scripts build pattern-post-type=./src/pattern-post-type.js",
		"format:js": "wp-scripts format src -- --config=../../../../.prettierrc.js",
		"lint:css": "echo \"No CSS.\"",
		"lint:js": "wp-scripts lint-js src",
		"start": "wp-scripts start pattern-post-type=./src/pattern-post-type.js"
	},
	"dependencies": {
		"@wordpress/components": "14.1.2",
		"@wordpress/data": "5.1.1",
		"@wordpress/edit-post": "4.1.4",
		"@wordpress/element": "3.1.1",
		"@wordpress/i18n": "4.1.1",
		"@wordpress/plugins": "3.1.1",
		"@wordpress/scripts": "16.1.2"
	},
	"eslintConfig": {
		"extends": "../../../../.eslintrc.js"
	},
	"stylelint": {
		"extends": "../../../../.stylelintrc"
	}
}
