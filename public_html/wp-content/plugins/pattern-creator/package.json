{
	"name": "wporg-pattern-creator",
	"version": "1.0.0",
	"description": "Create block patterns on the frontend of a site.",
	"author": "The WordPress Meta Team",
	"license": "GPL-2.0-or-later",
	"scripts": {
		"build": "wp-scripts build",
		"dev": "NODE_ENV=development wp-scripts build",
		"format:js": "wp-scripts format src -- --config=../../../../.prettierrc.js",
		"lint:css": "wp-scripts lint-style 'style.css' 'src/**/*.scss'",
		"lint:js": "wp-scripts lint-js src",
		"start": "wp-scripts start",
		"test:unit": "wp-scripts test-unit-js"
	},
	"devDependencies": {
		"@wordpress/dependency-extraction-webpack-plugin": "3.4.1",
		"@wordpress/scripts": "22.3.0",
		"autoprefixer": "10.4.4",
		"deep-freeze": "0.0.1",
		"postcss-custom-properties": "12.1.5"
	},
	"dependencies": {
		"@wordpress/a11y": "3.5.0",
		"@wordpress/api-fetch": "6.2.0",
		"@wordpress/block-editor": "8.4.0",
		"@wordpress/block-library": "7.2.0",
		"@wordpress/blocks": "11.4.0",
		"@wordpress/components": "19.7.0",
		"@wordpress/compose": "5.3.0",
		"@wordpress/core-data": "4.3.0",
		"@wordpress/data": "6.5.0",
		"@wordpress/dom-ready": "3.5.0",
		"@wordpress/edit-post": "6.2.0",
		"@wordpress/editor": "12.4.0",
		"@wordpress/element": "4.3.0",
		"@wordpress/format-library": "3.3.0",
		"@wordpress/hooks": "3.5.0",
		"@wordpress/i18n": "4.5.0",
		"@wordpress/icons": "8.1.0",
		"@wordpress/interface": "4.4.0",
		"@wordpress/keyboard-shortcuts": "3.3.0",
		"@wordpress/keycodes": "3.5.0",
		"@wordpress/notices": "3.5.0",
		"@wordpress/plugins": "4.3.0",
		"@wordpress/primitives": "3.3.0",
		"@wordpress/url": "3.6.0",
		"classnames": "2.3.1",
		"lodash": "4.17.21",
		"rememo": "4.0.0"
	},
	"eslintConfig": {
		"extends": "../../../../.eslintrc.js",
		"globals": {
			"wporgBlockPattern": "readonly",
			"wporgLocale": "readonly"
