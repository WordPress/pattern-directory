{
	"name": "wporg-pattern-directory-theme",
	"version": "0.0.0",
	"description": "Theme for wordpress.org/patterns",
	"author": "WordPress.org",
	"license": "GPL-2.0-or-later",
	"private": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/WordPress/pattern-directory/"
	},
	"scripts": {
		"start": "grunt watch",
		"build": "NODE_ENV=production grunt build",
		"build:css": "grunt css",
		"build:js": "grunt js",
		"dev": "grunt",
		"lint:css": "wp-scripts lint-style css",
		"lint:js": "wp-scripts lint-js src",
		"format:js": "wp-scripts format src -- --config=../../../../.prettierrc.js",
		"test:unit": "wp-scripts test-unit-js",
		"packages-update": "wp-scripts packages-update"
	},
	"browserslist": [
		"extends @wordpress/browserslist-config"
	],
	"devDependencies": {
		"@lodder/grunt-postcss": "3.0.1",
		"@wordpress/a11y": "3.2.1",
		"@wordpress/api-fetch": "5.2.1",
		"@wordpress/block-editor": "7.0.1",
		"@wordpress/browserslist-config": "4.1.0",
		"@wordpress/components": "16.0.0",
		"@wordpress/compose": "5.0.1",
		"@wordpress/core-data": "4.0.1",
		"@wordpress/data": "6.0.1",
		"@wordpress/data-controls": "2.2.2",
		"@wordpress/element": "4.0.0",
		"@wordpress/html-entities": "3.2.1",
		"@wordpress/i18n": "4.2.1",
		"@wordpress/icons": "5.0.1",
		"@wordpress/keycodes": "3.2.1",
		"@wordpress/primitives": "3.0.0",
		"@wordpress/scripts": "16.1.4",
		"@wordpress/url": "3.2.1",
		"@wordpress/viewport": "4.0.1",
		"autoprefixer": "10.3.3",
		"classnames": "2.3.1",
		"cssnano": "5.0.8",
		"grunt": "1.4.1",
		"grunt-contrib-clean": "2.0.0",
		"grunt-contrib-watch": "1.1.0",
		"grunt-sass": "3.1.0",
		"grunt-sass-globbing": "1.5.1",
		"grunt-webpack": "4.0.3",
		"lodash": "4.17.21",
		"postcss": "8.3.6",
		"react-use-gesture": "9.1.3",
		"rtlcss": "3.3.0",
		"sass": "1.38.2",
		"use-deep-compare-effect": "1.6.1"
	},
	"eslintConfig": {
		"extends": "../../../../.eslintrc.js",
		"globals": {
			"wporgLocale": "readonly",
			"wporgPatternsData": "readonly",
			"wporgPatternsUrl": "readonly"
		}
	},
	"stylelint": {
		"extends": "../../../../.stylelintrc",
		"ignoreFiles": [
			"**/*.css",
			"**/*.css.map"
		]
	},
	"dependencies": {}
}
