{
	"name": "wporg-pattern-directory-theme",
	"version": "0.0.0",
	"description": "Theme for wordpress.org/patterns",
	"author": "WordPress.org",
	"license": "GPL-2.0-or-later",
	"private": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/WordPress/pattern-directory/"
	},
	"scripts": {
		"start": "grunt watch",
		"build": "NODE_ENV=production grunt build",
		"build:css": "grunt css",
		"build:js": "grunt js",
		"dev": "grunt",
		"lint:css": "wp-scripts lint-style 'css/**/*.scss'",
		"lint:js": "wp-scripts lint-js src",
		"format:js": "wp-scripts format src -- --config=../../../../.prettierrc.js",
		"test:unit": "wp-scripts test-unit-js",
		"packages-update": "wp-scripts packages-update"
	},
	"browserslist": [
		"extends @wordpress/browserslist-config"
	],
	"devDependencies": {
		"@lodder/grunt-postcss": "3.1.1",
		"@wordpress/a11y": "3.29.0",
		"@wordpress/api-fetch": "6.26.0",
		"@wordpress/block-editor": "11.6.0",
		"@wordpress/browserslist-config": "5.12.0",
		"@wordpress/components": "23.6.0",
		"@wordpress/compose": "6.6.0",
		"@wordpress/core-data": "6.6.0",
		"@wordpress/data": "8.6.0",
		"@wordpress/data-controls": "2.29.0",
		"@wordpress/dom": "3.29.0",
		"@wordpress/element": "5.6.0",
		"@wordpress/html-entities": "3.29.0",
		"@wordpress/i18n": "4.29.0",
		"@wordpress/icons": "9.20.0",
		"@wordpress/keycodes": "3.29.0",
		"@wordpress/primitives": "3.27.0",
		"@wordpress/scripts": "26.0.0",
		"@wordpress/url": "3.30.0",
		"@wordpress/viewport": "5.6.0",
		"autoprefixer": "10.4.14",
		"classnames": "2.3.2",
		"cssnano": "5.1.15",
		"grunt": "1.5.3",
		"grunt-contrib-clean": "2.0.1",
		"grunt-contrib-watch": "1.1.0",
		"grunt-sass": "3.1.0",
		"grunt-sass-globbing": "1.5.1",
		"grunt-webpack": "5.0.0",
		"lodash": "4.17.21",
		"postcss": "8.4.21",
		"react-use-gesture": "9.1.3",
		"rtlcss": "4.0.0",
		"sass": "1.59.3",
		"use-deep-compare-effect": "1.8.1"
	},
	"eslintConfig": {
		"extends": "../../../../.eslintrc.js",
		"globals": {
			"wporgLocale": "readonly",
			"wporgPatternsData": "readonly",
			"wporgPatternsUrl": "readonly"
		}
	},
	"stylelint": {
		"extends": "../../../../.stylelintrc",
		"ignoreFiles": [
			"**/*.css",
			"**/*.css.map"
		]
	},
	"dependencies": {}
}
