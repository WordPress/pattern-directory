{
	"name": "wporg-pattern-directory-theme",
	"version": "0.0.0",
	"description": "Theme for wordpress.org/patterns",
	"author": "WordPress.org",
	"license": "GPL-2.0-or-later",
	"private": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/WordPress/pattern-directory/"
	},
	"scripts": {
		"start": "grunt watch",
		"build": "grunt build",
		"build:css": "grunt css",
		"build:js": "grunt js",
		"dev": "grunt",
		"lint:css": "wp-scripts lint-style css",
		"lint:js": "wp-scripts lint-js src",
		"format:js": "wp-scripts format-js src",
		"test:unit": "wp-scripts test-unit-js",
		"packages-update": "wp-scripts packages-update"
	},
	"browserslist": [
		"extends @wordpress/browserslist-config"
	],
	"devDependencies": {
		"@wordpress/a11y": "2.15.2",
		"@wordpress/api-fetch": "3.23.1",
		"@wordpress/block-editor": "5.3.0",
		"@wordpress/browserslist-config": "3.0.1",
		"@wordpress/components": "13.0.0",
		"@wordpress/compose": "3.25.0",
		"@wordpress/core-data": "2.26.0",
		"@wordpress/data": "4.27.0",
		"@wordpress/data-controls": "1.21.0",
		"@wordpress/element": "2.20.0",
		"@wordpress/i18n": "3.19.0",
		"@wordpress/icons": "2.10.0",
		"@wordpress/keycodes": "2.19.0",
		"@wordpress/primitives": "1.12.0",
		"@wordpress/scripts": "14.0.1",
		"@wordpress/url": "2.22.0",
		"@wordpress/viewport": "2.26.0",
		"autoprefixer": "9.8.6",
		"classnames": "2.3.1",
		"cssnano": "4.1.10",
		"grunt": "1.3.0",
		"grunt-contrib-watch": "1.1.0",
		"grunt-postcss": "0.9.0",
		"grunt-sass": "3.1.0",
		"grunt-sass-globbing": "1.5.1",
		"grunt-webpack": "4.0.2",
		"lodash": "4.17.21",
		"react-use-gesture": "9.1.3",
		"sass": "1.32.8"
	},
	"eslintConfig": {
		"extends": "../../../../.eslintrc.js",
		"globals": {
			"wporgAssetUrl": "readonly",
			"wporgSiteUrl": "readonly"
		}
	},
	"prettier": "../../../../.prettierrc.js",
	"stylelint": {
		"extends": "../../../../.stylelintrc",
		"ignoreFiles": [
			"**/*.css",
			"**/*.css.map"
		]
	},
	"dependencies": {}
}
